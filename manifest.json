{
	"manifest_version": 3,
	"name": "Google Meet Heartbeat",
	"version": "1.0",
	"permissions": [
		"tabs",
		"storage",
		"background"
	],
	"background": {
		"service_worker": "background.js"
	},
	"host_permissions": [
		"https://meet.google.com/*"
	],
	"options_page": "options.html",
	"icons": {
		"16": "logo16.png",
		"32": "logo32.png",
		"48": "logo48.png",
		"128": "logo128.png"
	}
}